<svg viewBox="0 0 400 400" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <!-- Glowing gradients -->
    <radialGradient id="glowCenter">
      <stop offset="0%" style="stop-color:#00D9FF;stop-opacity:0.6" />
      <stop offset="100%" style="stop-color:#00D9FF;stop-opacity:0" />
    </radialGradient>
    
    <linearGradient id="arcGradient" x1="0%" y1="0%" x2="100%" y2="0%">
      <stop offset="0%" style="stop-color:#00FFD9;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#00D9FF;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#0099FF;stop-opacity:1" />
    </linearGradient>
    
    <linearGradient id="powerGradient" x1="0%" y1="0%" x2="100%" y2="0%">
      <stop offset="0%" style="stop-color:#2D5016;stop-opacity:1" />
      <stop offset="25%" style="stop-color:#6B9F20;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#F4E03C;stop-opacity:1" />
      <stop offset="75%" style="stop-color:#F4A63C;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#E8592F;stop-opacity:1" />
    </linearGradient>
    
    <linearGradient id="rainbowGradient" x1="0%" y1="0%" x2="100%" y2="0%">
      <stop offset="0%" style="stop-color:cyan;stop-opacity:1" />
      <stop offset="25%" style="stop-color:magenta;stop-opacity:1" />
      <stop offset="50%" style="stop-color:yellow;stop-opacity:1" />
      <stop offset="75%" style="stop-color:cyan;stop-opacity:1" />
      <stop offset="100%" style="stop-color:magenta;stop-opacity:1" />
    </linearGradient>
    
    <!-- Blur filter for glow -->
    <filter id="glow">
      <feGaussianBlur stdDeviation="3" result="coloredBlur"/>
      <feMerge>
        <feMergeNode in="coloredBlur"/>
        <feMergeNode in="SourceGraphic"/>
      </feMerge>
    </filter>
    
    <!-- Subtle drop shadow -->
    <filter id="shadow">
      <feDropShadow dx="0" dy="2" stdDeviation="2" flood-color="#00D9FF" flood-opacity="0.3"/>
    </filter>
  </defs>
  
  <!-- Dark background with subtle grid -->
  <rect width="400" height="400" fill="#0A0E27"/>
  
  <!-- Animated grid background -->
  <g opacity="0.1">
    <line x1="0" y1="100" x2="400" y2="100" stroke="#00D9FF" stroke-width="0.5">
      <animate attributeName="opacity" values="0.05;0.15;0.05" dur="4s" repeatCount="indefinite"/>
    </line>
    <line x1="0" y1="200" x2="400" y2="200" stroke="#00D9FF" stroke-width="0.5">
      <animate attributeName="opacity" values="0.15;0.05;0.15" dur="4s" repeatCount="indefinite"/>
    </line>
    <line x1="0" y1="300" x2="400" y2="300" stroke="#00D9FF" stroke-width="0.5">
      <animate attributeName="opacity" values="0.05;0.15;0.05" dur="4s" repeatCount="indefinite"/>
    </line>
  </g>
  
  <g transform="translate(200, 180)">
    <!-- Central glow effect -->
    <circle cx="0" cy="0" r="100" fill="url(#glowCenter)" opacity="0.3">
      <animate attributeName="r" values="100;110;100" dur="3s" repeatCount="indefinite"/>
    </circle>
    
    <!-- Outer containment rings with pulse -->
    <circle cx="0" cy="0" r="155" fill="none" stroke="#00D9FF" stroke-width="1" opacity="0.3">
      <animate attributeName="r" values="155;157;155" dur="2s" repeatCount="indefinite"/>
    </circle>
    <circle cx="0" cy="0" r="150" fill="none" stroke="#00D9FF" stroke-width="2" opacity="0.5" filter="url(#glow)"/>
    

    
    <!-- Horizontal scan lines -->
    <g opacity="0.3">
      <line x1="-145" y1="-60" x2="145" y2="-60" stroke="#00D9FF" stroke-width="1">
        <animate attributeName="opacity" values="0.2;0.4;0.2" dur="2s" repeatCount="indefinite"/>
      </line>
      <line x1="-145" y1="0" x2="145" y2="0" stroke="#00D9FF" stroke-width="0.5">
        <animate attributeName="opacity" values="0.1;0.3;0.1" dur="3s" repeatCount="indefinite"/>
      </line>
      <line x1="-145" y1="60" x2="145" y2="60" stroke="#00D9FF" stroke-width="1">
        <animate attributeName="opacity" values="0.2;0.4;0.2" dur="2.5s" repeatCount="indefinite"/>
      </line>
    </g>
    
    <!-- Power level arc (sweeping around top) -->
    <path d="M -150,0 A 150,150 0 0,1 150,0" fill="none" stroke="url(#powerGradient)" 
          stroke-width="6" stroke-linecap="round"
          stroke-dasharray="35 436.97" filter="url(#glow)">
      <animate attributeName="stroke-dashoffset" 
               from="0" 
               to="-471.97" 
               dur="2.5s" 
               repeatCount="indefinite"/>
    </path>
    
    <!-- Static decorative arc underneath -->
    <path d="M -150,0 A 150,150 0 0,1 150,0" fill="none" stroke="url(#arcGradient)" 
          stroke-width="2" stroke-linecap="round" opacity="0.2"/>
    

    
    <!-- TORQUE text (refined) -->
    <g filter="url(#shadow)">
      <!-- T (with cross design) -->
      <path d="M -92 -20 L -52 -20 L -52 -14 L -75 -14 L -75 20 L -69 20 L -69 -14 L -92 -14 Z" fill="white"/>
      
      <!-- O -->
      <path d="M -45 0 Q -45 -22 -32 -22 Q -19 -22 -19 0 Q -19 22 -32 22 Q -45 22 -45 0 M -39 0 Q -39 16 -32 16 Q -25 16 -25 0 Q -25 -16 -32 -16 Q -39 -16 -39 0" fill="white"/>
      
      <!-- R -->
      <path d="M -11 20 L -11 -20 L 0 -20 Q 8 -20 8 -12 Q 8 -6 3 -4 L 10 20 L 4 20 L -2 -2 L -5 -2 L -5 20 Z M -5 -8 L -1 -8 Q 2 -8 2 -12 Q 2 -14 -1 -14 L -5 -14 Z" fill="white"/>
      
      <!-- Q -->
      <path d="M 18 0 Q 18 -22 31 -22 Q 44 -22 44 0 Q 44 16 36 20 L 41 26 L 37 29 L 31 22 Q 18 21 18 0 M 24 0 Q 24 16 31 16 Q 38 16 38 0 Q 38 -16 31 -16 Q 24 -16 24 0" fill="white"/>
      
      <!-- U -->
      <path d="M 52 -20 L 52 4 Q 52 22 64 22 Q 76 22 76 4 L 76 -20 L 70 -20 L 70 4 Q 70 16 64 16 Q 58 16 58 4 L 58 -20 Z" fill="white"/>
      
      <!-- E (electric spring) -->
      <g transform="translate(88, 0)">
        <path d="M 0 -20 L 0 -16 L 6 -12 L 0 -8 L 6 -4 L 0 0 L 6 4 L 0 8 L 6 12 L 0 16 L 0 20 L 14 20 L 14 15 L 5 15 L 5 13 L 11 9 L 5 5 L 11 1 L 5 -3 L 11 -7 L 5 -11 L 5 -15 L 14 -15 L 14 -20 Z" fill="white"/>
      </g>
    </g>
    
    <!-- Accent underline with gradient -->
    <line x1="-90" y1="30" x2="100" y2="30" stroke="url(#arcGradient)" stroke-width="1.5" opacity="0.6"/>
  </g>
  
  <!-- BOARD text with spacing -->
  <text x="200" y="255" font-family="'Arial', sans-serif" font-size="26" font-weight="300" fill="white" text-anchor="middle" letter-spacing="14" opacity="0.95" filter="url(#shadow)">BOARD</text>
  
  <!-- Animated data stream indicators -->
  <g opacity="0.4">
    <rect x="60" y="340" width="3" height="12" fill="#00D9FF">
      <animate attributeName="height" values="8;18;8" dur="1s" repeatCount="indefinite"/>
    </rect>
    <rect x="68" y="340" width="3" height="12" fill="#00D9FF">
      <animate attributeName="height" values="12;8;16" dur="1.2s" repeatCount="indefinite"/>
    </rect>
    <rect x="76" y="340" width="3" height="12" fill="#00D9FF">
      <animate attributeName="height" values="14;10;14" dur="0.9s" repeatCount="indefinite"/>
    </rect>
    
    <rect x="318" y="340" width="3" height="12" fill="#00D9FF">
      <animate attributeName="height" values="10;16;10" dur="1.1s" repeatCount="indefinite"/>
    </rect>
    <rect x="326" y="340" width="3" height="12" fill="#00D9FF">
      <animate attributeName="height" values="8;14;8" dur="1s" repeatCount="indefinite"/>
    </rect>
    <rect x="334" y="340" width="3" height="12" fill="#00D9FF">
      <animate attributeName="height" values="12;8;18" dur="1.3s" repeatCount="indefinite"/>
    </rect>
  </g>
  
  <!-- Bottom tagline with glowing effect -->
  <text x="200" y="375" font-family="'Courier New', monospace" font-size="10" font-weight="400" fill="#00D9FF" text-anchor="middle" letter-spacing="3" opacity="0.8" filter="url(#glow)">ADVANCED TESLA CONTROLS AND ANALYTICS</text>
</svg>